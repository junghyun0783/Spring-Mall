<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<!-- Type 별칭 미리 정의하기 -->
	<typeAliases>
		<typeAlias type="test.gura.computer.product.dto.ProductDto" alias="productDto"/>
		<typeAlias type="test.gura.computer.Members.dto.MembersDto" alias="membersDto"/>
		<typeAlias type="test.gura.computer.qna.dto.QAWriterDto" alias="QAWriterDto"/>
		<typeAlias type="test.gura.computer.qna.dto.QAAnswerDto" alias="qaanswerDto"/>
		<typeAlias type="test.gura.computer.evaluation.dto.EvaluationDto" alias="evaluationDto"/>
		<typeAlias type="test.gura.computer.evaluation.dto.EvaluationCommentDto" alias="evaluationCommentDto"/>
		<typeAlias type="test.gura.computer.cart.dto.CartDto" alias="cartDto"/>
	</typeAliases>

	<!-- sql 문을 정의하고 있는 xml 문서 정보 -->
	<mappers>
		<mapper resource="test/gura/computer/mybatis/ProductMapper.xml"/>
		<mapper resource="test/gura/computer/mybatis/MembersMapper.xml"/>
		<mapper resource="test/gura/computer/mybatis/QAWriterMapper.xml"/>
		<mapper resource="test/gura/computer/mybatis/QAAnswerMapper.xml"/>
		<mapper resource="test/gura/computer/mybatis/EvaluationMapper.xml"/>
		<mapper resource="test/gura/computer/mybatis/EvaluationCommentMapper.xml"/>
		<mapper resource="test/gura/computer/mybatis/CartMapper.xml"/>
	</mappers>
</configuration>




